
    @model LoginPageRegistrationPage.Models.Register
   
<!DOCTYPE html>
<html>
<head>
    <title>My Profile DashBoard</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="utf-8">
    <meta name="keywords" content="Square Profile Widget a Flat Responsive Widget,Login form widgets, Sign up Web forms , Login signup Responsive web form,Flat Pricing table,Flat Drop downs,Registration Forms,News letter Forms,Elements" />

    <link href="css/fontawesome-all.min.css" rel="stylesheet" type="text/css" media="all">
    <link href="css/style.css" rel='stylesheet' type='text/css' />

    <!--webfonts-->

    <link href="//fonts.googleapis.com/css?family=Roboto:400,500,700,900" rel="stylesheet">
    <!--//webfonts-->

</head>
<body>

    <!-- main -->

    <div class="center-container">

        <div class="w3ls_banner_info">


            <h1>My Profile  </h1>
            <div class="profile">
                <div class="wrap">
                    <div class="profile-main">
                        <div class="profile-pic wthree">
                            @foreach (var item in Model.Registersinfo)
                            {
                                <img src="~/images/@item.ProfilePicture" height="100" width="90"
                                         asp-append-version="true" alt="">
                                <h3>@Html.DisplayFor(modelitem => item.FirstName) </h3>
                                <div class="w3ls-agile-left">
                                    <!---728x90--->

                                    <ul class="address">
                                        <li>
                                            <ul class="address-text">
                                                <li><b>Gender</b></li>
                                                <li>: </li>
                                                <li> <span class="w3">@Html.DisplayFor(modelitem => item.Gender)</span></li>
                                            </ul>
                                        </li>

                                        <li>
                                            <ul class="address-text">
                                                <li><b>Password</b></li>
                                                <li>: </li>
                                                <li> <span class="w3">@Html.DisplayFor(modelitem => item.Password)</span></li>
                                            </ul>
                                        </li>
                                        <li>
                                            <ul class="address-text">
                                                <li><b>Contact No.</b></li>
                                                <li>: </li>
                                                <li> <span class="w3">@Html.DisplayFor(modelitem => item.PhoneNumber)</span></li>
                                            </ul>
                                        </li>
                                        <li>
                                            <ul class="address-text">
                                                <li><b>Email</b></li>
                                                <li>: </li>
                                                <li> <span class="w3">@Html.DisplayFor(modelitem => item.Email)</span></li>
                                            </ul>
                                        </li>
                                        <li>
                                            <ul class="address-text">
                                                <li><b>Security Question</b></li>
                                                <li>: </li>
                                                <li><span class="w3">@Html.DisplayFor(modelitem => item.SecurityQuestion)</span></li>
                                            </ul>
                                        </li>
                                        <li>
                                            <ul class="address-text">
                                                <li><b>Security Answer</b></li>
                                                <li>: </li>
                                                <li><span class="w3">@Html.DisplayFor(modelitem => item.SecurityAnwser)</span></li>
                                            </ul>
                                        </li>

                                    </ul>
                                    <div class="clear"></div>
                                </div>
                            }
                        </div>

                      
                        <div class="clear"> </div>
                        <div class="profile-follows">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- //main -->

    <script src="js/jquery.min.v3.js"></script>
    <script src="js/jquery.circlechart.js"></script>
    <script>
        $('.demo-1').percentcircle();

        $('.demo-2').percentcircle({
            animate: false,
            diameter: 100,
            guage: 3,
            coverBg: '#000',
            bgColor: '#ff0000',
            fillColor: '#000',
            percentSize: '50px',
            percentWeight: 'normal'
        });


    </script>
    <script type="text/javascript">

        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-36251023-1']);
        _gaq.push(['_setDomainName', 'jqueryscript.net']);
        _gaq.push(['_trackPageview']);

        (function() {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();

    </script>


</body>
</html>